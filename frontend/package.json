{
  "name": "news-bias-analyzer-frontend",
  "version": "0.1.0",
  "description": "Frontend for the News Bias Analyzer platform",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github-pages": "tsc && VITE_GITHUB_PAGES=true VITE_APP_ENV=production vite build",
    "build:production": "tsc && VITE_APP_ENV=production vite build",
    "preview": "vite preview",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "axios": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  }
}